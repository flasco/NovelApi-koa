{
  "site": "https://www.soxs.cc",
  "charset": "UTF-8",
  "chapterList": ".novel_list dd a",
  "chapterTitle": ".read_title h1",
  "chapterContent": ".content",
  "chapterError": "正在手打中",
  "search": {
    "method": "post",
    "pattern": "https://www.soxs.cc/search.html?searchtype=all&searchkey=${key}",
    "line": "#main ul li",
    "chapter": ".s2 a",
    "author": ".s4"
  },
  "detail": {
    "latest": "meta[property='og:novel:latest_chapter_name']",
    "description": "meta[property='og:description']",
    "imageUrl": "meta[property='og:image']",
    "author": "meta[property='og:novel:author']",
    "name": "meta[property='og:novel:book_name']"
  }
}
